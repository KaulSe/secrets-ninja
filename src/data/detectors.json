{
    "Stripe": {
        "endpoints": {
            "account_metadata": {
                "label": "Account Metadata",
                "curl": "curl https://api.stripe.com/v1/account -u sk_live_xxxx:",
                "request_url": "https://api.stripe.com/v1/account",
                "request_method": "GET"
            },
            "balance": {
                "label": "Balance",
                "curl": "curl https://api.stripe.com/v1/balance -u sk_live_xxxx:",
                "request_url": "https://api.stripe.com/v1/balance",
                "request_method": "GET",
                "override_default_input_field": true,
                "input_fields": {
                    "api_key": {
                        "type": "text",
                        "label": "Enter Stripe API Key",
                        "placeholder": "sk_live_xxxx"
                    }
                }
            },
            "customers": {
                "label": "Customers",
                "curl": "curl https://api.stripe.com/v1/customers -u sk_live_xxxx:",
                "request_url": "https://api.stripe.com/v1/customers",
                "request_method": "GET"
            },
            "orders": {
                "label": "Orders",
                "curl": "curl https://api.stripe.com/v1/orders -u sk_live_xxxx:",
                "request_url": "https://api.stripe.com/v1/orders",
                "request_method": "GET"
            },
            "payment_intents": {
                "label": "Payment Intents",
                "curl": "curl https://api.stripe.com/v1/payment_intents -u sk_live_xxxx:",
                "request_url": "https://api.stripe.com/v1/payment_intents",
                "request_method": "GET"
            }
        },
        "input_fields": {
            "api_key": {
                "type": "text",
                "label": "Enter Stripe API Key",
                "placeholder": "sk_live_xxxx"
            }
        }
    },
    "Paypal": {
        "endpoints": {
            "get_access_scope": {
                "label": "Get Access Scope",
                "curl": "curl -v -X GET https://api.sandbox.paypal.com/v1/oauth2/token/userinfo?schema=openid -H \"Content-Type: application/json\" -H \"Authorization: Bearer A21AAHc",
                "request_url": "https://api.sandbox.paypal.com/v1/oauth2/token/userinfo?schema=openid",
                "request_method": "GET"
            },
            "get_payments": {
                "label": "Get Balance",
                "curl": "curl -v -X GET https://api.paypal.com/v1/payments/payment -H \"Content-Type: application/json\" -H \"Authorization: Bearer A21AAHc",
                "request_url": "https://api.paypal.com/v1/payments/payment",
                "request_method": "GET"
            }
        },
        "input_fields": {
            "access_token": {
                "type": "text",
                "label": "Enter Access Token",
                "placeholder": "A21AAHc"
            },
            "access_token_secret": {
                "type": "text",
                "label": "Enter Access Token Secret",
                "placeholder": "A21AAHc Secret"
            }
        },
        "alert": {
            "alert_message": "Currently only the access scopes endpoint works.",
            "color": "info"
        }
    },
    "OpenAI": {
        "endpoints": {
            "organizations": {
                "label": "Get Organizations",
                "curl": "curl -X GET https://api.openai.com/v1/organizations",
                "request_url": "https://api.openai.com/v1/organizations",
                "request_method": "GET"
            }
        },
        "input_fields": {
            "api_key": {
                "type": "text",
                "label": "Enter OpenAI API Key",
                "placeholder": "sk-xxxxx-xxxxx-xxxxx-xxxxx"
            }
        }
    },
    "Paystack": {
        "endpoints": {
            "transaction": {
                "label": "Get Transaction",
                "curl": "curl https://api.paystack.co/transaction",
                "request_url": "https://api.paystack.co/transaction",
