    "Paypal": {
        "endpoints": {
            "get_access_scope": {
                "label": "Get Access Scope",
                "curl": "curl -X POST 'https://api.paypal.com/v1/oauth2/token' -u 'access_token:access_secret' -d 'grant_type=client_credentials'",
                "request_url": "https://api.paypal.com/v1/oauth2/token",
                "request_method": "GET"
            },
            "get_payments": {
                "label": "Get Payments",
                "curl": "access_token=$(curl -s -X POST 'https://api.paypal.com/v1/oauth2/token' -u 'access_token:access_secret' -d 'grant_type=client_credentials' | jq -r '.access_token') && curl -X GET 'https://api.paypal.com/v1/payments/payment' -H 'Authorization: Bearer $access_token'",
                "request_url": "https://api.paypal.com/v1/payments/payment",
                "request_method": "GET"
            }
        },
        "input_fields": {
            "client_id": {
                "type": "text",
                "label": "Enter Client ID",
                "placeholder": "A21AAHc"
            },
            "client_secret": {
                "type": "text",
                "label": "Enter Client Secret",
                "placeholder": "A21AAHc Secret"
            }
        }
    },
