        "input_fields": {
            "api_key": {
                "type": "text",
                "label": "Enter Paystack API Key",
                "placeholder": "sk_live_xxxx"
            }
        }
    },
    "Github": {
        "endpoints": {
            "user": {
                "label": "Get User",
                "curl": "curl https://api.github.com/user",
                "request_url": "https://api.github.com/user",
                "request_method": "GET"
            },
            "repos": {
                "label": "Get Repos",
