            },
            "orders": {
                "label": "Get Orders",
                "curl": "curl -X GET",
                "request_url": "https://api.omnisend.com/v3/orders?limit=100",
                "request_method": "GET"
            },
            "campaigns": {
                "label": "Get Campaigns",
                "curl": "curl -X GET",
                "request_url": "https://api.omnisend.com/v3/campaigns?limit=100",
                "request_method": "GET"
            }
        },
        "input_fields": {
            "api_key": {
                "type": "text",
                "label": "Enter Omnisend API Key",
                "placeholder": "xxx-xxxxx-xxxxx-xxxxx-xxxxx"
            }
        }
    },
    "Shopify": {
        "endpoints": {
            "orders": {
                "label": "Get Orders",
                "curl": "curl -X GET",
                "request_url": "https://shopify.com/admin/api/2023-10/orders.json",
                "request_method": "GET"
            },
            "customers": {
                "label": "Get Customers",
                "curl": "curl -X GET",
                "request_url": "https://shopify.com/admin/api/2023-10/customers.json",
                "request_method": "GET"
            },
            "orders_count": {
                "label": "Get Orders Count",
                "curl": "curl -X GET",
                "request_url": "https://shopify.com/admin/api/2023-10/orders/count.json",
                "request_method": "GET"
            },
            "customers_count": {
                "label": "Get Customers Count",
                "curl": "curl -X GET",
                "request_url": "https://shopify.com/admin/api/2023-10/customers/count.json",
                "request_method": "GET"
            }
        },
        "input_fields": {
            "api_key": {
                "type": "text",
                "label": "Enter Shopify API Key",
                "placeholder": "shp_12345"
            },
            "store_domain": {
                "type": "text",
                "label": "Enter Store Domain",
                "placeholder": "store.myshopify.com"
            }
        },
        "alert": {
            "alert_message": "Shopify API Checker is not available for now. Please try again later.",
            "color": "failure"
        }
    },
    "Telegram": {
        "endpoints": {
            "bot_info": {
                "label": "Get Bot Info",
                "curl": "curl -X GET https://api.telegram.org/bot<token>/getMe",
                "request_url": "https://api.telegram.org/bot<token>/getMe",
                "request_method": "GET"
            }
        },
        "input_fields": {
            "bot_token": {
                "type": "text",
                "label": "Enter Bot Token",
                "placeholder": "123456:ABC-DEF1234ghIkl-zyx57W2v1u123ew11"
            }
        }
    },
    "Clearbit": {
        "endpoints": {
            "person": {
                "label": "Get Person",
                "curl": "curl 'https://person.clearbit.com/v2/people/find?email=test@test.com' -u sk_xxx-xxx-xxx:",
                "request_url": "https://person.clearbit.com/v2/people/find?email=",
                "request_method": "GET",
                "override_default_input_field": true,
                "input_fields": {
                    "api_key": {
                        "type": "text",
                        "label": "Enter Clearbit API Key",
                        "placeholder": "sk_xxx"
                    },
                    "email": {
                        "type": "text",
                        "label": "Enter Email",
                        "placeholder": "test@test.com"
                    }
                }
            }
        },
        "input_fields": {
            "api_key": {
                "type": "text",
                "label": "Enter Clearbit API Key",
                "placeholder": "sk_xxx"
            }
        },
        "alert": {
            "alert_message": "Can't work because of CORS. Use the curl on your local machine to test it.",
            "color": "failure"
        }
    }
}